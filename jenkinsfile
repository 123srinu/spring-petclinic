pipeline {
  agent none
  tools { 
        maven 'Maven'
        jdk 'JAVA_HOME'
  }
  stages {
    stage('Clone repository') {
    agent { label "NewNodes" }
      steps {
        checkout scm
      }
    }
  }
  stages {
  parallel{
      stage('Build') {
      agent { label "NewNode" }
        steps {
        sh 'mvn clean package'
        sh 'echo $USER'
        sh 'echo whoami'
        }
      },
      stage('forDemo') {
      agent { label "NewNode" }
      steps {
        sh 'echo Sudhakar'
        sh 'sleep 100'
      }
    }
   }
 }
}
