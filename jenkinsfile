
pipeline {
  agent none
  tools { 
        maven 'Maven'
        jdk 'JAVA_HOME'
  }
  environment {
        MYJOB = 'DevOps'
        BUILD = 'PIPELINE'
    }
  stages {
    stage('Clone repository') {
    agent { label "NewNodes" }
	environment {
        MYJOB = 'DevOps1'
        BUILD = 'PIPELINE1'
    }
      steps {
        checkout scm
        sh 'echo MYJOB'
      }
    }
      stage('Build') {
      agent { label "NewNode" }
        steps {
        sh 'mvn clean package'
        sh 'echo $USER'
        sh 'echo whoami'
        sh 'echo MYJOB'
        }
      }
      stage('forDemo') {
      agent { label "NewNode" }
      steps {
        sh 'echo Sudhakar'
        sh 'sleep 100'
      }
    }
   }
}

